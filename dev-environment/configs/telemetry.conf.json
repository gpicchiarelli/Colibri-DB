{
  "enabled": true,
  "collectionInterval": 1.0,
  "exportInterval": 60.0,
  "exportDirectory": "./dev-environment/reports/telemetry",
  "exportFormats": ["json", "csv", "prometheus"],
  "retentionPeriod": 86400,
  "collectMetrics": true,
  "collectPerformance": true,
  "collectErrors": true,
  "collectQueries": true,
  "collectSystem": true,
  "collectMemory": true,
  "collectIO": true,
  "collectTransactions": true,
  "collectIndexes": true,
  "compressionEnabled": false,
  "logLevel": "debug",
  "maxMemoryEntries": 20000,
  "realTimeMonitoring": true,
  "anomalyDetection": true,
  "trendAnalysis": true,
  "correlationAnalysis": true,
  "predictionAnalysis": false
}
